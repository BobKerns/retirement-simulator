{
    "name": "retirement-simulator",
    "version": "0.1.0",
    "description": "Retirement Simulator",
    "main": "lib/cjs/index.js",
    "module": "lib/esm/index.js",
    "browser": "lib/umd/index.js",
    "jsdelivr": "lib/umd/index.js",
    "unpkg": "lib/umd/index.js",
    "types": "lib/esm/index.d.ts",
    "scripts": {
        "build:compile": "tsc --build config src src/__tests__",
        "pretest": "npm run build:compile",
        "test": "jest",
        "prebuild": "npm run build:compile",
        "build": "rollup -c",
        "postbuild": "npm run build:docs",
        "prewatch": "npm run build:compile",
        "watch": "NODE_ENV=development rollup -c --watch",
        "clean": "rm -rf build lib docs",
        "prepublishOnly": "NODE_ENV=production npm run build",
        "prebuild:docs": "npm install",
        "build:docs": "GRAPHVIZ_DOT=\"$(which dot)\" typedoc --out build/docs/api src/",
        "preserve": "npm run build:compile",
        "serve": "serve -l 3035 -s",
        "predocs": "npm run build:compile && npm run build:docs",
        "docs": "npm run serve"
    },
    "bin": {},
    "repository": {
        "type": "git",
        "url": "git+https://github.com/BobKerns/retirement-simulator.git"
    },
    "keywords": [
        "retirement",
        "finance",
        "observablehq",
        "typescript",
        "observablehq"
    ],
    "author": "Bob Kerns",
    "license": "MIT",
    "bugs": {
       "url": "https://github.com/BobKerns/retirement-simulator/issues"
    },
    "homepage": "https://github.com/BobKerns/retirement-simulator#readme",
    "devDependencies": {
        "@rollup/plugin-commonjs": "^19.0.2",
        "@rollup/plugin-node-resolve": "^13.0.4",
        "@types/jest": "^26.0.24",
        "jest": "^27.0.6",
        "jest-mock-proxy": "^3.0.0",
        "mermaid": "^8.11.1",
        "rollup": "^2.55.0",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-visualizer": "^5.5.2",
        "serve": "^12.0.0",
        "terser": "^5.7.1",
        "ts-jest": "^27.0.4",
        "typedoc": "^0.21.4",
        "typedoc-plugin-mermaid": "^1.4.0",
        "typedoc-plugin-npm-externals": "^1.0.0",
        "typescript": "^4.3.5",
        "wcsize": "^1.0.0",
        "@rollup/plugin-json": "^4.1.0",
        "@types/d3": "^7.0.0",
        "@types/heap": "^0.2.28",
        "@types/istanbul-reports": "^3.0.1",
        "ramda": "^0.27.1",
        "@types/ramda": "^0.27.44"
    },
    "dependencies": {
        "@observablehq/plot": "^0.1.0",
        "d3": "^7.0.0",
        "genutils": "^0.1.41",
        "heap": "^0.2.6"
    }
}
