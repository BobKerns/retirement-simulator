{
  "cleanUrls": false,
  "trailingSlash": true,
  "etag": true,
  "headers": [
      {
        "source": "**/*.@(csv|json)",
        "headers" : [{
            "key" : "Access-Control-Allow-Origin",
            "value" : "*"
        },
        {
            "key": "Cache-Control",
            "value": "max-age=0, must-revalidate"
        }]
    }]
}
